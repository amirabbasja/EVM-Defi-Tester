deployArbSep:
	forge script script/UniswapV2Deployer.s.sol --rpc-url arbitrum_sepolia --keystore ~/.foundry/keystores/main --broadcast --verify

seeMainWalletAddress:
	cast wallet address --keystore ~/.foundry/keystores/main

